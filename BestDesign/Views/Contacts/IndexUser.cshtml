@model IEnumerable<BestDesign.Models.Contact>

@{
    ViewData["Title"] = "Index";
}


<div class="page-header" style="background: url(assets/img/banner1.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="breadcrumb-wrapper">
                    <h2 class="product-title">Testimonial</h2>
                    <ol class="breadcrumb">
                        <li><a href="#">Home /</a></li>
                        <li class="current">Testimonial</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>


<section class="services section-padding">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {


                <div class="col-md-6 col-lg-4 col-xs-12">
                    <div class="services-item wow fadeInRight animated" data-wow-delay="0.2s" style="visibility: visible;-webkit-animation-delay: 0.2s; -moz-animation-delay: 0.2s; animation-delay: 0.2s;">
                        <div class="icon">
                            <i class=""></i>
                        </div>
                        <div class="services-content">
                            <h3>@item.Name</h3>
                            <p>@item.Message</p>
                        </div>
                    </div>
                </div>

            }
        </div>
    </div>
</section>


